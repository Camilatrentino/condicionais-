algoritmo "maior que 10"
// Função :
// Autor :
// Data : 18/05/2023
// Seção de Declarações 
var
num: real
resultado: logico

inicio

//entrada

escreval("digite o número: ")
leia (num)

//processamento

se (num > 10)entao
 escreval("Seu número é maior que 10")

senao
 escreval("Seu numero é menor que 10 ")
 
fimse

fimalgoritmo
