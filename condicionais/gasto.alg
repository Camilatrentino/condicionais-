algoritmo "gasto"
// Função :
// Autor :
// Data : 18/05/2023
// Seção de Declarações 
var
sala, gasto: real



inicio

//entrada

escreval("digite o seu salário: ")
leia (sala)

escreval("digite o seu gasto mensal: ")
leia (gasto)

//processamento

se (sala >= gasto)entao
 escreval("Saldo positivo ")

senao
 escreval("Saldo negativo ")
 
fimse

fimalgoritmo
